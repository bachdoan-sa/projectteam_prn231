@page
@model WebAppRazorpage.Areas.Admin.Pages.DealHanger.CreateModel
@{
    Layout = "_AdminLayout";
}
<h1>Create DealHanger</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="Model.DealStatus"></label>
        <input asp-for="Model.DealStatus" class="form-control" />
        <span asp-validation-for="Model.DealStatus" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Model.Currency"></label>
        <input asp-for="Model.Currency" class="form-control" />
        <span asp-validation-for="Model.Currency" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Model.CustomerId"></label>
        <input asp-for="Model.CustomerId" class="form-control" />
        <span asp-validation-for="Model.CustomerId" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Model.AuctionStateId"></label>
        <input asp-for="Model.AuctionStateId" class="form-control" />
        <span asp-validation-for="Model.AuctionStateId" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Create DealHanger</button>
</form>